{
  "order": [
    "OTP",
    "U2F",
    "YubiX",
    "Software_Projects"
  ],
  "project": "projects",
  "template": {
    "define": {
      "github": {
        "releases": {
          "dir": "Releases",
          "abortIfEmpty": true
        },
        "git": [
          {
            "url": "https://github.com/Yubico/%(name)s.git",
            "files": [
              ["README", "index.adoc"],
              ["doc/*", "./"]
            ]
          }, {
            "url": "https://github.com/Yubico/yubico.github.com.git",
            "files": [["%(name)s/releases/*", "Releases/"]]
          }
        ],
        "links": {
          "*": [
            {
              "name": "Github",
              "url": "https://github.com/Yubico/%(name)s"
            }
          ]
        }
      }
    }
  }
}
