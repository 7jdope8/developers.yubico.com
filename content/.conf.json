{
  "order": [
    "OTP",
    "U2F",
    "PIV",
    "Software_Projects"
  ],
  "hidden": ["404", "PGP"],
  "project": "projects",
  "template": {
    "define": {
      "github": {
        "order": ["Releases", "Release_Notes"],
        "releases": {
          "dir": "Releases",
          "abortIfEmpty": true
        },
        "git": [
          {
            "url": "https://github.com/Yubico/%(name)s.git",
            "files": [
              ["README", "index.adoc"],
              ["NEWS", "Release_Notes.adoc"],
              ["doc/*", "./"],
              ["*.png", "./"]
            ]
          }, {
            "url": "https://github.com/Yubico/yubico.github.com.git",
            "files": [["%(name)s/releases/*", "Releases/"]]
          }
        ],
        "links": [
          {
            "name": "<i class=\"fa fa-github fa-lg\"></i> Github",
            "url": "https://github.com/Yubico/%(name)s"
          }
        ]
      }
    }
  }
}
